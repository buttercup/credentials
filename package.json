  "dependencies": {
    "@buttercup/signing": "^0.1.0",
    "iocane": "^0.10.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1",
    "prettier-check": "^2.0.0"
  }
